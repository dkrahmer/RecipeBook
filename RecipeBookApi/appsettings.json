{
	"AWS": {
		"Region": "us-east-1",
		"Profile": "default"
	},
	"AllowedOrigins": "https://your-web-domain.com",

	"GoogleClientId": "___________",
	"GoogleClientSecret": "___________",
	// Generate Google client ID: https://developers.google.com/identity/sign-in/web/sign-in

	"MySqlConnectionString": "server=___________; database=___________; user=___________; password=___________; convert zero datetime=True",

	"AlwaysDecimalUnits": [ "g", "mg", "kg", "L", "ml" ],

	"UnitEquivalents": [ // The first unit for each array is the preferred name (pluralization will be automatically corrected)
		[ "g", "gm", "grams", "gram" ],
		[ "kg", "kgs", "kilograms", "kilogram" ],
		[ "oz", "ounce", "ounces" ],
		[ "lbs", "lb", "#", "pounds", "pound" ],
		[ "tsp", "teaspoons", "teaspoon", "ts", "t", "tspn", "tspns" ],
		[ "tblsp", "tblsps", "tbls", "tbl", "tb", "tbs", "tbsp", "tbsps", "T", "Ts", "tablespoon", "tablespoons" ],
		[ "cups", "cup", "c", "cp" ],
		[ "qts", "qt", "qrt", "qrts", "quart", "quarts" ],
		[ "gallons", "gals", "gal", "gallon", "gln", "glns" ],
		[ "ml", "milliliter", "milliliters", "millilitre", "millilitres" ],
		[ "L", "ltr", "ltrs", "liter", "liters", "litre", "litres" ]
	],

	"UnitAppropriations": [
		{ // Rules and ConversionEquations use DataTable.Compute() to evaluate
			"InputUnit": "oz",
			"Rules": [ "{{value}} >= 16", "CONVERT({{value}} * 100, 'System.Int32') % (4 * 100) = 0" ],
			"ConversionEquation": "{{value}} / 16", // 1/16
			"OutputUnit": "lbs"
		},
		{
			"InputUnit": "lbs",
			"Rules": [ "{{value}} < 1" ],
			"ConversionEquation": "{{value}} * 16",
			"OutputUnit": "oz"
		},
		{
			"InputUnit": "tsp",
			"Rules": [ "CONVERT({{value}} * 100, 'System.Int32') % CONVERT(1.5 * 100, 'System.Int32') = 0" ], // evenly divisible by 1.5
			"ConversionEquation": "{{value}} / 3",
			"OutputUnit": "tblsp"
		},
		{
			"InputUnit": "tsp",
			"Rules": [ "{{value}} >= 6" ], // prevent high tsp count
			"ConversionEquation": "{{value}} / 3",
			"OutputUnit": "tblsp"
		},
		{
			"InputUnit": "tblsp",
			"Rules": [ "{{value}} >= 4", "CONVERT({{value}} * 100, 'System.Int32') % (4 * 100) = 0" ],
			"ConversionEquation": "{{value}} / (4 * 4)",
			"OutputUnit": "cups"
		},
		{
			"InputUnit": "tblsp",
			"Rules": [ "{{value}} < 2", "CONVERT({{value}} * 100, 'System.Int32') % CONVERT(0.5 * 100, 'System.Int32') <> 0" ], // not evenly divisible by 0.5
			"ConversionEquation": "{{value}} * 3",
			"OutputUnit": "tsp"
		},
		{
			"InputUnit": "tblsp",
			"Rules": [ "{{value}} >= 2" ], // prevent high tblsp count
			"ConversionEquation": "{{value}} / (4 * 4)",
			"OutputUnit": "cups"
		},
		{
			"InputUnit": "cups",
			"Rules": [ "{{value}} < (1/8)" ],
			"ConversionEquation": "{{value}} * (4 * 4)",
			"OutputUnit": "tblsp"
		},
		{
			"InputUnit": "cups",
			"Rules": [ "{{value}} >= 4", "CONVERT({{value}} * 100, 'System.Int32') % CONVERT(2 * 100, 'System.Int32') <> 0" ],
			"ConversionEquation": "{{value}} / 4",
			"OutputUnit": "qts"
		},
		{
			"InputUnit": "cups",
			"Rules": [ "{{value}} >= 16" ],
			"ConversionEquation": "{{value}} / 4",
			"OutputUnit": "qts"
		},
		{
			"InputUnit": "qts",
			"Rules": [ "{{value}} >= 6" ],
			"ConversionEquation": "{{value}} / 4",
			"OutputUnit": "gal"
		},
		{
			"InputUnit": "g",
			"Rules": [ "{{value}} >= 1000" ],
			"ConversionEquation": "{{value}} / 1000",
			"OutputUnit": "kg"
		}
	],

	"MetricConversions": [
		{
			"InputUnit": "tsp",
			"ConversionEquation": "{{value}} * 5", // 4.92892
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "tblsp",
			"ConversionEquation": "{{value}} * 15", // 14.7868
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "cups",
			"ConversionEquation": "{{value}} * 240", // 236.588
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "qts",
			"ConversionEquation": "{{value}} * 950", // 946.353
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "gallons",
			"ConversionEquation": "{{value}} * 3800", // 3785.41
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "ml",
			"Rules": [ "{{value}} >= 1000" ],
			"ConversionEquation": "{{value}} / 1000",
			"OutputUnit": "L"
		},
		{
			"InputUnit": "L",
			"Rules": [ "{{value}} < 1" ],
			"ConversionEquation": "{{value}} * 1000",
			"OutputUnit": "ml"
		},
		{
			"InputUnit": "lbs",
			"ConversionEquation": "{{value}} * 450", // 453.592
			"OutputUnit": "g"
		},
		{
			"InputUnit": "oz",
			"ConversionEquation": "{{value}} * 30", // 28.3495
			"OutputUnit": "g"
		},
		{
			"InputUnit": "g",
			"Rules": [ "{{value}} >= 1000" ],
			"ConversionEquation": "{{value}} / 1000",
			"OutputUnit": "kg"
		},
		{
			"InputUnit": "kg",
			"Rules": [ "{{value}} < 1" ],
			"ConversionEquation": "{{value}} * 1000",
			"OutputUnit": "g"
		}
	],

	"VolumeToMassConversionMinMl": 20, // Do not convert to mass if equal or less than this value

	"VolumeToMassConversions": { // name, 1 ml, g
		"butter": 0.955,
		"milk": 1.035,
		"water": 1,
		"sugar": 0.85,
		"brown sugar": 0.93,
		"flour": 0.55,
		"salt": 1.217,
		//"baking soda": ???
		"baking powder": 0.972,
		"peanut butter": 1.01,
		"buttermilk": 1.023,
		"oil": 0.921,
		"cream cheese": 0.951,
		"powdered sugar": 0.473,
		"sweetened condensed milk": 1.293,
		"heavy cream": 1.014,
		"olive oil": 0.9,
		"dry milk": 0.287,
		"cocoa powder": 0.507,
		"cornstarch": 0.507,
		"almond flour": 0.406,
		"cornmeal": 0.676,
		"corn flour": 0.755,
		"sour cream": 1.036,
		"whole wheat flour": 0.593
	}
}
