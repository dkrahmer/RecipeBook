		{ "Names": ["corn flour"], "Density": 0.755 },
		{ "Names": ["cornmeal","corn meal"], "Density": 0.676 },
		{ "Names": ["cornstarch", "corn starch"], "Density": 0.507 },
		{ "Names": ["cream cheese"], "Density": 0.951 },
		{ "Names": ["dry milk", "nonfat dry milk"], "Density": 0.287 },
		{ "Names": ["flour", "white flour", "AP flour", "all-purpose flour"], "Density": 0.55 },
		{ "Names": ["heavy cream", "heavy whipping cream"], "Density": 1.014 },
		{ "Names": ["milk", "2% milk"], "Density": 1.035 },
		{ "Names": ["oil", "vegetable oil", "canola oil"], "Density": 0.921 },
		{ "Names": ["olive oil"], "Density": 0.916 },
		{ "Names": ["peanut butter"], "Density": 1.08 },
		{ "Names": ["powdered sugar", "confectioners sugar"], "Density": 0.473 },
		{ "Names": ["salt"], "Density": 1.15 },
		{ "Names": ["sour cream"], "Density": 1.036 },
		{ "Names": ["sugar", "white sugar", "granulated sugar"], "Density": 0.85 },
		{ "Names": ["sweetened condensed milk", "condensed milk"], "Density": 1.293 },
		{ "Names": ["water"], "Density": 1 },
		{ "Names": ["whole milk"], "Density": 1.026 },
		{ "Names": ["whole wheat flour"], "Density": 0.593 }		//"baking soda": ???
	]
}
